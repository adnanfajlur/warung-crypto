{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "no-console": 0,
        "import/prefer-default-export": 0,
        "no-use-before-define": 0,
        "import/first": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0,
        "max-len": 0,
        "no-extra-boolean-cast": 0,
        "operator-assignment": 0
    }
}